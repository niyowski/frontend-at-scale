{
  "name": "microfrontends-with-single-spa",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "prepare": "husky install",
    "format": "nx format:write",
    "lint": "nx affected:lint --fix",
    "start:root": "yarn nx run @acme/root-config:start",
    "start:ui-header": "yarn nx run @acme/ui-header:start",
    "start:ui-footer": "yarn nx run @acme/ui-footer:start",
    "start:mf-auth": "yarn nx run @acme/mf-auth:start"
  },
  "dependencies": {
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.16.0",
    "single-spa": "^5.9.3",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@nx/eslint-plugin": "^16.9.1",
    "@nx/js": "16.9.1",
    "@nx/linter": "^16.9.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.7.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^9.1.7",
    "jest": "^27.0.6",
    "jest-cli": "^27.0.6",
    "lint-staged": "^14.0.1",
    "nx": "16.9.1",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2"
  },
  "workspaces": [
    "apps/*",
    "libs/*",
    "packages/*"
  ]
}
